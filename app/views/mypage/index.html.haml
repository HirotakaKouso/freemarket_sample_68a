= render "shared/header"

%body
  #wrapper
    .wrapper__content
      .wrapper__content__pankuzu
        %a{href: "#"}> ホーム
        ＞
        %a ユーザーSHOP
      .wrapper__content__main
        -# プロフィール編集ページへ
        %a.wrapper__content__main__editProfBtn{href: "#", rel: ""} プロフィール編集
        .wrapper__content__main__name
          %img.wrapper__content__main__name__picProf{alt: "profileimage", src: "https://image.shoppies.jp/img_m/noimage-profile.png"}/
          %h1.wrapper__content__main__name__shopName 自分の名前（お店の名前）
          %p.wrapper__content__main__name__nickName ニックネーム
        .wrapper__content__main__shop
          %p.wrapper__content__main__shopDesc
          -# ショップ詳細ページへ移動
          %a.wrapper__content__main__shopInfoBtn{href: "#"}
            詳細ページ移動
            -# 評価一覧画面へ
        %a.wrapper__content__main__evaluations{href: "#"}
          %p 評価：
          %img.wrapper__content__main__evaluationsIcon{alt: "goodsmile", src: "https://image.shoppies.jp/img_m/icon_good.png"}/
          %p.wrapper__content__main__evaluationsGood 良い
          %p.wrapper__content__main__evaluationsCnt 0
          %img.wrapper__content__main__evaluationsSmile{alt: "normalsmile", src: "https://image.shoppies.jp/img_m/icon_normal.png"}/
          %p.wrapper__content__main__evaluationsNormal ふつう
          %p.wrapper__content__main__evaluationsCnt 0
          %img.wrapper__content__main__evaluationsSmile{alt: "badsmile", src: "https://image.shoppies.jp/img_m/icon_bad.png"}/
          %p.wrapper__content__main__evaluationsBad 悪い
          %p.wrapper__content__main__evaluationsCnt 0
          %p.wrapper__content__main__evaluationsDealCnt 取引数：0
          -# ボタンを押したらいいねでカウントが増える
        .wrapper__content__main__likeBtn
          %input.wrapper__content__main__likeBtnShop{alt: "likebtn", src: "https://image.shoppies.jp/img_m/suki_off.png", type: "image", value: "1"}/
        .wrapper__content__main__likeCnt
          0
      %br
      .wrapper__content__exhibition
        -# 出品ページへ
        %a.wrapper__content__exhibition__ItemBtn{:href => "#"} 出品する

      .wrapper__content__page
        .wrapper__content__page__select
          %nav.wrapper__content__page__select__navi
            %ul
              %li.prev
                %span.active.prevTxt 前へ
              %li
                %span 1
              %li
                %a{href: "/user-shop/20937015/p/2"} 2
              %li
                %a{href: "/user-shop/20937015/p/3"} 3
              %li
                %a{href: "/user-shop/20937015/p/4"} 4
              %li.last
                %a{href: "/user-shop/20937015/p/5"} …5
              %li.next
                %a{href: "/user-shop/20937015/p/2"} 次へ


